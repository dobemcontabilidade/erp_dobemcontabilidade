        <addForeignKeyConstraint baseColumnNames="enquadramento_id"
                                 baseTableName="empresa"
                                 constraintName="fk_empresa__enquadramento_id"
                                 referencedColumnNames="id"
                                 referencedTableName="enquadramento"
                                 />

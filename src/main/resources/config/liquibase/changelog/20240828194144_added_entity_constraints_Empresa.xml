    <!--
        Added the constraints for entity Empresa.
    -->
    <changeSet id="20240828194144-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="pessoa_juridica_id"
                                 baseTableName="empresa"
                                 constraintName="fk_empresa__pessoa_juridica_id"
                                 referencedColumnNames="id"
